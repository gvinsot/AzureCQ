{
  "name": "azurecq",
  "version": "1.1.0",
  "description": "Azure Cheap Queues is a High-performance queue system based on Redis and Azure Storage with message durability guarantees",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/__tests__/integration.test.ts'",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.ts' --testPathIgnorePatterns='integration.test.ts'",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "example:basic": "npx ts-node examples/basic-usage.ts",
    "example:queue-mgmt": "npx ts-node examples/queue-management.ts",
    "example:dlq": "npx ts-node examples/dead-letter-queue.ts",
    "example:azure-comparison": "npx ts-node examples/azure-storage-comparison.ts",
    "example:hot-path": "npx ts-node examples/hot-path-optimization.ts",
    "example:test-duplicate-fix": "npx ts-node examples/test-duplicate-fix.ts"
  },
  "keywords": [
    "queue",
    "redis",
    "azure",
    "storage",
    "message",
    "batch",
    "performance"
  ],
  "author": "Gildas",
  "license": "MIT",
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@azure/storage-queue": "^12.16.0",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
